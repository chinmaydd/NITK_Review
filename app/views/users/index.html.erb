<div class="users index">
  <h2>Users</h2>

  <table summary="Users list">
    <tr class="header">
      <th>User Name</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to( user.username , {action: 'show', id: user.id}) %></td>
    </tr>
    <% end %>
  </table>

<%= link_to("Back to home", {action: 'home'}) %>

<%= link_to("Logout", {:action => 'logout' })  %>
<br>
<br>
<br>
</div>
